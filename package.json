    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix -c .eslintrc-import-order.json",
      "prettier --write",
      "eslint --max-warnings 0",
