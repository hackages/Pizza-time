{
  "name": "hj-hooks-pizza",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^7.2.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.5",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
